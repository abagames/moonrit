  "license": "MIT",
  "devDependencies": {
    "cross-conf-env": "^1.1.2",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-clean-dist": "0.0.6",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "gif-capture-canvas": "^1.0.1",
    "soundfont-player": "^0.11.0",
    "tonal": "^2.1.2"
  }
}
